{
  "name": "Byggelig",
  "description": "Et byggelig prosjekt",
  "version": "0.2.0",
  "author": "Netlife Design",
  "dependencies": {
    "@sanity/block-content-to-react": "^2.0.6",
    "classnames": "^2.2.6",
    "gatsby": "^2.4.3",
    "gatsby-image": "^2.0.41",
    "gatsby-plugin-google-tagmanager": "^2.0.13",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-offline": "^2.1.1",
    "gatsby-plugin-purgecss": "^4.0.0",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-react-svg": "^2.1.1",
    "gatsby-plugin-sharp": "^2.0.37",
    "gatsby-plugin-sitemap": "^2.1.0",
    "gatsby-source-filesystem": "^2.0.33",
    "gatsby-source-sanity": "^4.0.1",
    "gatsby-transformer-sharp": "^2.1.19",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-waypoint": "^9.0.2",
    "zenscroll": "^4.0.2"
  },
  "devDependencies": {
    "@netlify/zip-it-and-ship-it": "^0.3.1",
    "netlify-lambda": "^1.4.13",
    "link-checker": "^1.2.1",
    "babel-eslint": "^9.0.0",
    "eslint": "^5.16.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.5.0",
    "gatsby-plugin-sass": "^2.0.11",
    "node-sass": "^4.12.0",
    "prettier": "^1.17.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build && npm run link-checker && npm run lambda-build && npm run move-redirect",
    "link-checker": "npx link-checker ./public --external-only --json --file-ignore static --http-redirects 16 | node linkChecker.js",
    "develop": "gatsby develop",
    "move-redirect": "cp _redirects public/",
    "dev": "gatsby develop",
    "lambda": "netlify-lambda serve src/serverless",
    "lambda-build": "cd src/serverless && npm install && cd ../.. && zip-it-and-ship-it src/serverless serverless-build",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
